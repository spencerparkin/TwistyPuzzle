    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch TwistyPuzzle app",
            "type": "cppdbg",
            "request": "launch",
            "MIMode": "gdb",
            "program": "${workspaceFolder}/build/Bin/TwistyPuzzle",
            "cwd": "${workspaceFolder}",
            "environment": [
                {"name": "GDK_BACKEND", "value": "x11"}
            ],
            "externalConsole": true
        }
    ]
}